//- the model, the data
-
    var header = '#what I can do';
    var whatCanIDoList = [
        {
            title: 'Front End',
            skillList: [
                'Unicorn Trainer',
                'Cooking',
                'React',
                'Redux',
                'HTML5',
                'CSS3',
                'Galaxy defender',
                'UltraSuperMan',
                'Flower picker'
            ]
        },
        {
            title: 'Back End',
            skillList: [
                'FireBase',
                'Git',
                'CLI',
                'Pupet master',
                'Virtual dreamer',
                'Spit lava angry dude'
            ]
        }
    ];

//- the presentation, the view
#what-can-i-do
.what-can-i-do
    .what-can-i-do__header=header
    each whatCanIDo in whatCanIDoList
        .what-can-i-do__title=whatCanIDo.title
        .what-can-i-do__list
            each skill in whatCanIDo.skillList
                .what-can-i-do__skill=skill